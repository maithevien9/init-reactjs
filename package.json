{
  "name": "Web3",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run check-types && vite build",
    "start": "vite preview --port 3000",
    "check-types": "tsc --noEmit --pretty",
    "check-types:watch": "tsc --noEmit --watch",
    "lint": "eslint \"src/**/*.{tsx,ts}\" --fix",
    "lint-staged": "lint-staged",
    "lint-pass": "echo '\\033[4;32m♡ ' Format complete ♡' \\033[0m'",
    "prepare": "husky install",
    "test": "vitest run",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@ant-design/pro-card": "^2.0.27",
    "@ant-design/pro-layout": "6.38.18",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@enouvo/react-hooks": "^0.0.1",
    "@enouvo/react-uikit": "^0.0.13",
    "antd-img-crop": "^4.5.1",
    "axios": "^1.2.0",
    "dayjs": "^1.11.6",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-router-dom": "6.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@enouvo/eslint-config-react": "^1.5.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-react": "^2.2.0",
    "antd": "^4.24.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "less": "^4.1.3",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rc-picker": "^2.7.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-antd-dayjs": "^1.0.0",
    "vite-plugin-imp": "^2.3.1",
    "vite-plugin-pages": "^0.27.1",
    "vite-plugin-sentry": "^1.1.6",
    "vite-plugin-svgr": "^2.2.2",
    "vitest": "^0.24.5"
  },
  "eslintIgnore": [
    "vite.config.ts",
    "scripts/*",
    "country-schemas.tsx",
    "src/vite-env.d.ts"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
